<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.traffic.Mapper.FetchData">
    <insert id="storePOIs">
        INSERT INTO factory(id,name,address,latitude,longitude)
        VALUES (#{id},#{name},#{address},#{latitude},#{longitude})
    </insert>

    <select id="POIList">
        select * from factory;
    </select>
</mapper>